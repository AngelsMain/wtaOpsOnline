{
  "name": "vueaxios",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve;webpack --env.API_URL=http://localhost:8000 --config webpack.config.dev.js",
    "build": "vue-cli-service build;webpack --env.API_URL=https://www.myapi.com --config webpack.config.build.js",
    "lint": "vue-cli-service lint;webpack --env.API_URL=http://localhost:8000 --config webpack.config.dev.js",
  },
  "dependencies": {
    "axios": "^0.18.0",
    "mixins": "0.0.1",
    "qs": "^6.6.0",
    "vee-validate": "^2.1.6",
    "vue": "^2.5.13",
    "vue-axios": "^2.0.2",
    "vue-router": "^3.0.1",
    "vue-session": "^1.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-beta.1",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.1",
    "@vue/cli-service": "^3.0.0-beta.1",
    "vue-template-compiler": "^2.5.13"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
